            <a onclick="rotateLeft()"><i class="fa fa-undo" aria-hidden="true"></i></a>
            <a onclick="initialPosition()"><i class="fa fa-picture-o" aria-hidden="true"></i></a>
            <a onclick="rotateRight()"><i class="fa fa-repeat" aria-hidden="true"></i></a>
        </div>*@

    @*<pre>{{poll || json}}</pre>*@
</div>
